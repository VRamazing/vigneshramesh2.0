{
  "name": "vigneshramesh",
  "version": "1.0.0",
  "description": ",y official website",
  "main": "index.js",
  "scripts": {
    "start-all-dev": "npm-run-all --parallel start-server-dev sass-build-watch",
    "start-prod": "node server.js",
    "start-server-dev": "nodemon --watch views --watch scripts server.js",
    "uglify-js": "uglifyjs public/scripts/data.js public/scripts/main.js  -o public/scripts/data.min.js",
    "sass-build": "sass public/styles/main.scss public/styles/main.css --style=compressed",
    "sass-build-watch": "sass public/styles/main.scss public/styles/main.css --style=compressed --watch"
  },
  "keywords": [
    "vignesh ramesh",
    "vramazing",
    "official website",
    "handlebars"
  ],
  "author": "vignesh ramesh",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "sass": "^1.76.0",
    "uglify-js": "^3.17.4"
  }
}
