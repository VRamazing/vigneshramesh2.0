 <!--contact-->
    <div class="container contact" id="Contact">
      <div class="row ">
        <div class="col s12">
          <center>
            <img src="images/message.png" height="250" alt="contact_vignesh">
          </center>
        </div>
      </div>

      <br />

      <div class="row">
        <form class="col s12" action="https://formspree.io/techie.vigneshramesh@gmail.com"  method="Post">
          <div class="row">
            <div class="input-field col m6 s12">
              <input id="name" name="name" type="text" class="validate" required />
              <label for="name" class="blue-text">Name</label>
            </div>
            <div class="input-field col s12 m6">
              <input id="email" name="email" type="email" class="validate" required />
              <label for="email" class="blue-text">Email</label>
            </div>
          </div>


          <div class="row">
            <div class="input-field col s12">
              <textarea id="message" name="message" class="materialize-textarea" required></textarea>
              <label for="message" class="blue-text">Message</label>
            </div>
          </div>
          <br>
          <button class="btn waves-effect waves-dark blue contact-submit-btn" type="submit" name="action">Send
            <i class="fa fa-paper-plane" aria-hidden="true" style = "margin-left:10px;"></i>
          </button>
        </form>
      </div>
</div>