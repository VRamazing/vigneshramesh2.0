<!-- Header Banner -->
<div>
  <div class="section no-pad-bot scrollspy" id="Home">
  </div>

  <div class="about-header">
    <div class="intro-text">
      <div class="img-container">
        <img alt="profile_pic" src="./images/me.png" class="profilePic center" />
      </div>
      <div class="row center">
        <h1 class="white-text">Vignesh Ramesh</h1>
        <h4 class="white-text">Sr. Software Engineer &amp; Tech Evangelist</h4>
        <a href="#About" id="link" class="scroll-arrow"><i class="material-icons"
            aria-hidden="true">keyboard_arrow_down</i></a>
      </div>
    </div>
  </div>

  <!-- Quote -->
  <div class="container-fluid quote blue z-depth-1">
    <div class="row ">
      <div class="col s12">
        <br>
        <p class="center white-text">Leveraging JavaScript &amp; Python Expertise to Drive Innovation &amp; Optimize
          Performance.</p>
      </div>
    </div>
  </div>

  <!-- About Me -->
  <div class="container scrollspy" id="About">
    <div class="section about">
        <div class="experience row">
          <div class="about-sub-category">
            <i class="material-icons">keyboard</i>
            <span>Experience</span>
          </div>
          <ul class="row experience-list">
            <li>Innovative Software Engineer with 7+ years of creating and managing software and leading cross-functional teams.</li>
            <li> Skilled in javascript and python frameworks and technologies.</li>
            <li> Regularly exceed performance targets and possess advanced analytical and problem-solving skills.</li>
            <li>Adept at leveraging cutting-edge digital tools and platforms to achieve software development objectives.</li>
          </ul>
        </div>

          <br />

          <div class="technology row">
            <div class="about-sub-category">
              <i class="material-icons">whatshot</i>
              <span>Technology Stack</span>
            </div>
              <div class="icon-row">
                <div class="icon">
                  <i class="devicon-css3-plain dev-icon"></i>
                  <span>CSS</span>
                </div>

                <div class="icon">
                  <i class="devicon-html5-plain dev-icon"></i>
                  <span>HTML 5</span>
                </div>

                <div class="icon">
                  <i class="devicon-javascript-plain dev-icon"></i>
                  <span>JavaScript</span>
                </div>

                <div class="icon">
                  <i class="devicon-react-original dev-icon"></i>
                  <span>React js</span>
                </div>

                  <div class="icon">
                  <i class="devicon-nodejs-plain dev-icon"></i>
                  <span>Node js</span>
                </div>

              <div class="icon">
                  <i class="devicon-mongodb-plain dev-icon"></i>
                  <span>mongo db</span>
                </div>

                <div class="icon">
                  <i class="devicon-express-original dev-icon"></i>
                  <span>express js</span>
                </div>

                <div class="icon">
                  <i class="devicon-tailwindcss-original dev-icon"></i>
                  <span>Tailwind css</span>
                </div>

                <div class="icon">
                  <i class="devicon-python-plain dev-icon"></i>
                  <span>Python</span>
                </div>

                <div class="icon">
                  <i class="devicon-mysql-original dev-icon"></i>
                  <span>SQL</span>
                </div>
              </div>
          </div>

          <div class="divider divider-reviews"></div>


          <div class="reviews row">
            <div class="about-sub-category">
              <i class="material-icons">rate_review</i>
              <span>Reviews</span>
            </div>
            <div class="row">
              {{#each testimonials }}
              <div class="col s12 m4">
               <blockquote class="blue-text reviews">
                <div class="img-center">
                  <img src={{authorImg}} alt={{author}}>
                  <span >{{author}}</span>
                </div>
                
                <p class="flow-text">{{content}}<i class="material-icons" aria-hidden="true">format_quote
</i></p>
              </blockquote>
              </div>
              {{/each}}
            </div>
          </div>



  </div>
</div>
</div>

<div> <br />

</div>
</div>